<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chen.activity">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.chen.mobilesafe.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chen.mobilesafe.activity.HomeActivity"/>
        <activity android:name="com.chen.mobilesafe.activity.SettingActivity"/>
        <activity android:name="com.chen.mobilesafe.activity.TestActivity"/>
        <activity android:name="com.chen.mobilesafe.activity.SetupOverActivity"/>
        <activity android:name="com.chen.mobilesafe.activity.Setup1Activity"/>
        <activity android:name="com.chen.mobilesafe.activity.Setup2Activity"/>
        <activity android:name="com.chen.mobilesafe.activity.Setup3Activity"/>

    </application>

</manifest>